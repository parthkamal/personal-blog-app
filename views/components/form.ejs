<form class="contact-form" action="/contact" method="post">
    <label for="title">Title:</label>
    <input type="text" name="title" placeholder="title"/>
    <label for="email" name="from">Email Adress:</label>
    <input type="text" name="from" placeholder="email-address"/>
    <label for="body" name="description">Description:</label>
    <textarea name="body" id="body" cols="30" rows="10" placeholder="enter your description here"></textarea>
 <input type="submit" value="Submit">
</form>